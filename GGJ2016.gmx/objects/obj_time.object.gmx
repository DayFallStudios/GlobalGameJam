        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// every frame do:

time += 0.10; // after 1 second, time = 3;
if (time==12) drawn_frame=false;
if (time==30) drawn_frame=false;
if (time==60) drawn_frame=false;
if (time==90) drawn_frame=false;
if (time==120) drawn_frame=false;
if (time==138) drawn_frame=false;
if (time==150) drawn_frame=false;

y_change = 19;
// If first four seconds of daytime (Early morning)
if ((time &lt; 12) &amp;&amp; !(drawn_frame)) {
    obj_daylog.y += y_change;
    text_logged = false;
    background_colour = make_color_rgb(230,200,100);
    timeframe = "Early morning: ";
    drawn_frame = true;
}
// If next 6 seconds (Morning)
else if ((time &gt;= 12) &amp;&amp; (time &lt; 30) &amp;&amp; !(drawn_frame)) {
    obj_daylog.y += y_change;
    text_logged = false;
    background_colour = make_color_rgb(230,220,150);
    timeframe = "Morning: ";
    drawn_frame = true;
}
// If next 10 seconds (Before noon)
else if ((time &gt;= 30) &amp;&amp; (time &lt; 60) &amp;&amp; !(drawn_frame)) {
    obj_daylog.y += y_change;
    text_logged = false;
    background_colour = make_color_rgb(170,220,220);
    timeframe = "Before noon: ";
    drawn_frame = true;
}
// If next 10 seconds (Noon)
else if ((time &gt;= 60) &amp;&amp; (time &lt; 90) &amp;&amp; !(drawn_frame)) {
    obj_daylog.y += y_change;
    text_logged = false;
    background_colour = make_color_rgb(70,150,255);
    timeframe = "Noon: ";
    drawn_frame = true;
}
// If next 10 seconds (Afternoon)
else if ((time &gt;= 90) &amp;&amp; (time &lt; 120) &amp;&amp; !(drawn_frame)) {
    obj_daylog.y += y_change;
    text_logged = false;
    background_colour = make_color_rgb(100,170,255);
    timeframe = "Afternoon: ";
    drawn_frame = true;
}
// If next 6 seconds (Late afternoon)
else if ((time &gt;= 120) &amp;&amp; (time &lt; 138) &amp;&amp; !(drawn_frame)) {
    obj_daylog.y += y_change;
    text_logged = false;
    background_colour = make_color_rgb(255,200,100);
    timeframe = "Late afternoon: ";
    drawn_frame = true;
}
// If next 4 seconds (Dusk)
else if ((time &gt;= 138) &amp;&amp; (time &lt; 150) &amp;&amp; !(drawn_frame)) {
    obj_daylog.y += y_change;
    text_logged = false;
    background_colour = make_color_rgb(255,100,20);
    timeframe = "Dusk: ";
    drawn_frame = true;
}
// If next 10 seconds (Evening)
else if ((time &gt;= 150) &amp;&amp; (time &lt; 180) &amp;&amp; !(drawn_frame)) {
    obj_daylog.y += y_change;
    text_logged = false;
    background_colour = make_color_rgb(0,0,100);
    timeframe = "Evening: ";
    drawn_frame = true;
}
else {
    //time = 0;
    // Choice time
}
</string>
